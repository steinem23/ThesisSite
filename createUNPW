CREATE TABLE user (
id int(4) NOT NULL auto_increment,
uname varchar(25) NOT NULL,
fname varchar(25) NOT NULL,
lname varchar(25) NOT NULL,
pw varchar(25) NOT NULL,
isAdmin BIT NOT NULL, 
CONSTRAINT PRIMARY KEY (uname)
);

